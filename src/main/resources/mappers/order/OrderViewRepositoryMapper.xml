<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.ginong.web.repository.order.OrderViewRepository">
	
	<select id="findByOrderId" resultType="OrderView">
		select
		*
		from ORDER_VIEW
		where order_id = #{orderId}
	</select>

</mapper>