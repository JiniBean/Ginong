<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.ginong.web.repository.member.MemberRoleRepository">

    <select id = "findAllByMemberId" resultType="MemberRole">
        select
        *
        from MEMBER_ROLE
        where MEMBER_ID = #{memberId}
    </select>

</mapper>